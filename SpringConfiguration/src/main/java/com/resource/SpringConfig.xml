<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
 http://www.springframework.org/schema/context
 http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:annotation-config/>
	


	<bean id="emp" class="com.app.bean.Employee" >
		<property name="eid" value="101" />
		<property name="name" value="Pawan" />
		<property name="email" value="Pawamn@gmail.com" />

	</bean>

	<bean  class="com.app.bean.Address">
		<property name="pincode" value="2789" />
		<property name="area" value="Deoria" />
		<property name="distric" value="Deoriaup" />

	</bean>


</beans>
